[
  {
    "id": "git-push",
    "execute-command": "/bin/dagger",
    "include-command-output-in-response": true,
    "command-working-directory": "/hooks-test",
    "response-headers":
    [
      {
        "name": "Access-Control-Allow-Origin",
        "value": "*"
      }
    ],
    "pass-arguments-to-command":
    [
      {
        "source": "string",
        "name": "call"
      },
      {
        "source": "string",
        "name": "container-echo"
      },
      {
        "source": "string",
        "name": "--string-arg"
      },
      {
        "source": "payload",
        "name": "commits.0.commit.message"
      }
    ],
    "trigger-rule":
    {
      "and":
      [
        {
          "match":
          {
            "type": "value",
            "value": "refs/heads/main",
            "parameter":
            {
              "source": "payload",
              "name": "ref"
            }
          }
        }
      ]
    }
  }
]
